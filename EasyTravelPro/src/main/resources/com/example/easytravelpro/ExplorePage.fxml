<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPanel" prefHeight="598.0" prefWidth="764.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.easytravelpro.ExplorePageController">
   <children>
      <ImageView fitHeight="31.0" fitWidth="31.0" layoutX="1.0" layoutY="149.0" onMouseClicked="#activePics" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Icons/nextLeft.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fitHeight="31.0" fitWidth="31.0" layoutX="732.0" layoutY="149.0" onMouseClicked="#activePics" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Icons/nextright.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label fx:id="villaLabel" layoutX="32.0" layoutY="15.0" onMouseClicked="#activePics" prefHeight="300.0" prefWidth="700.0">
         <graphic>
            <ImageView fitHeight="300.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ExplorePagePhotos/villa.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      <Label fx:id="tentLabel" layoutX="32.0" layoutY="15.0" opacity="0.0" prefHeight="300.0" prefWidth="700.0" visible="false">
         <graphic>
            <ImageView fitHeight="300.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ExplorePagePhotos/tent.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
   </children>
</AnchorPane>
