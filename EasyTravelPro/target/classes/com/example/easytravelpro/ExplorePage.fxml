<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="mainPanel" prefHeight="619.0" prefWidth="764.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.easytravelpro.ExplorePageController">
   <children>
      <Label fx:id="villaLabel" layoutX="32.0" layoutY="15.0" onMouseClicked="#activePics" prefHeight="300.0" prefWidth="700.0">
         <graphic>
            <ImageView fitHeight="300.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ExplorePagePhotos/villa.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      <Label fx:id="tentLabel" layoutX="32.0" layoutY="15.0" opacity="0.0" prefHeight="300.0" prefWidth="700.0" visible="false">
         <graphic>
            <ImageView fitHeight="300.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ExplorePagePhotos/tent.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      <ScrollPane fx:id="scroll" layoutX="57.0" layoutY="328.0" prefHeight="271.0" prefWidth="647.0">
         <content>
            <HBox fx:id="roomBox" alignment="CENTER_LEFT" prefHeight="250.0" prefWidth="644.0" spacing="5.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
